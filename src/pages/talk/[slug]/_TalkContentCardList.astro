---
import { findManyTalk } from '#@/feature/talk/repository/findManyTalk.js'
import { CardGrid, LinkCard } from '@astrojs/starlight/components'

const slides = await findManyTalk()
---

<CardGrid>{slides.map(slide => <LinkCard href={`/talk/${slide.slug}/article`} title={slide.data.title} />)}</CardGrid>
